{% extends "information-page.njk" %}
{% from "govuk/components/button/macro.njk" import govukButton %}

{% set title = 'Tell us about each badger sett the work will affect' %}

{% block topNavigation %}
  <p class="govuk-body sdds-save-link">
      <a class="govuk-link govuk-link--no-visited-state" href="/tasklist">Save and return later</a>
  </p>
{% endblock %}

{% block pageContent %}
  <p class="govuk-body">You can add more than one sett.</p>

  {{ govukButton({
    text: 'Add the first sett',
    name: 'continue',
    attributes: { id: 'continue' },
    preventDoubleClick: true
  }) }}
  
{% endblock %}
